---
import '../styles/global.css';
import members from '../data/members.json';
---

<div class="members-grid">
    {
        members.map(member => (
            <div class="member-card">
                <img src={member.picture} alt={member.name} />
                <h3>{member.name}</h3>
                <p>{member.role}</p>
            </div>
        ))
    }
</div>